{
  "name": "@bernankez/utils",
  "type": "module",
  "version": "0.2.2",
  "packageManager": "pnpm@8.9.0",
  "description": "Cole's utils preset",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/utils.git"
  },
  "bugs": "https://github.com/Bernankez/utils/issues",
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "dev": "unbuild --stub && pnpm docs:dev",
    "build": "unbuild",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build --force",
    "docs:preview": "vitepress preview",
    "update": "tsx metadata/update.ts",
    "start": "tsx functions/index.ts",
    "watch": "tsx watch functions/index.ts",
    "release": "bumpp && pnpm publish",
    "test": "vitest run",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . && stylelint \"**/*.{css,vue}\" \".vitepress/**/*.{css,vue}\" \"!.vitepress/cache\" \"!.vitepress/dist\"",
    "fix": "eslint . --fix && stylelint \"**/*.{css,vue}\" \".vitepress/**/*.{css,vue}\" \"!.vitepress/cache\" \"!.vitepress/dist\" --fix"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.7.1",
    "@bernankez/eslint-config-unocss": "^0.7.1",
    "@bernankez/stylelint-config": "^0.4.4",
    "@iconify/json": "^2.2.128",
    "@types/node": "^20.8.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "gray-matter": "^4.0.3",
    "lint-staged": "^14.0.1",
    "rimraf": "^5.0.5",
    "simple-git": "^3.20.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.56.5",
    "vite": "^4.4.11",
    "vitepress": "1.0.0-rc.20",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.19"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "pnpm run update",
      "eslint --fix",
      "pnpm typecheck",
      "pnpm test"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
